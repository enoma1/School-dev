#include<stdio.h>

int maximum(int, int, int, int, int, int, int, int, int);

int main()
{
    int a, b, c, d, e, f, g, h, i;
    
    printf("enter nine integers integers");
    scanf("%d,%d,%d,%d,%d,%d,%d,%d,%d",&a,&b,&c,&d,&e,&f,&g,&h,&i);
    printf("maximum is %d\n",maximum(a,b,c,d,e,f,g,h,i));
    
    return 0;
}
int maximum(int r,int s,int t,int u,int v,int w,int x,int y,int z)
{
       int max = r;
  
    if (s > max)
     max = s;
    
    if (t > max)
     max = t;
    
    if (u > max)
     max = u;
   
    if (v > max)
     max = v;
   
    if (w > max)
     max = w;
 
    if (y > max)
     max = y;
    
    if (z > max)
        max = z;
    return max;
}
